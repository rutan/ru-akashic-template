{
  "name": "@rutan/akashic-template",
  "version": "1.0.0",
  "author": "ru_shalm <ru_shalm@hazimu.com>",
  "license": "Unlicense",
  "private": true,
  "scripts": {
    "start": "akashic-sandbox ./game",
    "serve": "akashic serve --target-service nicolive ./game",
    "build": "run-s lint clean build:* scan",
    "build:webpack": "cross-env NODE_ENV=production webpack",
    "build:web": "node ./scripts/buildForWeb.cjs",
    "watch": "webpack -w",
    "assets": "run-s assets:* scan",
    "assets:mkdir": "mkdirp ./tmp && mkdirp src/assets && mkdirp game/assets/textures",
    "assets:updateAssets": "node ./scripts/convertAssets.cjs",
    "scan": "cd game && akashic scan asset",
    "clean": "run-p clean:*",
    "clean:script": "rimraf ./game/script/bundle.js",
    "storybook": "start-storybook -p 6006 -s ./game",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -c .",
    "lint:tsc": "tsc --noEmit",
    "format": "run-s format:*",
    "format:eslint": "eslint --fix src",
    "format:prettier": "prettier --write .",
    "zip": "run-s build zip:*",
    "zip:archive": "node ./scripts/archive.cjs",
    "prepare": "husky install",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@akashic-extension/akashic-hover-plugin": "^3.2.2",
    "@akashic-extension/resolve-player-info": "^1.1.1",
    "@akashic/akashic-cli-commons": "^0.14.18",
    "@akashic/akashic-cli-scan": "^0.16.26",
    "@akashic/akashic-engine": "^3.16.4",
    "eventmit": "^2.0.4"
  },
  "devDependencies": {
    "@akashic/akashic-cli": "^2.16.53",
    "@akashic/akashic-cli-export": "^1.8.30",
    "@akashic/akashic-engine-standalone": "^3.16.3",
    "@akashic/akashic-sandbox": "^0.28.3",
    "@kudohamu/storybook-akashic": "^6.3.13",
    "@rutan/deployment-zip": "^0.2.1",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "cpx2": "^7.0.1",
    "cross-env": "^7.0.3",
    "esbuild-loader": "^4.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-rulesdir": "^0.2.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.2",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "wrap-ansi": "^7.0.0"
  },
  "pnpm": {
    "overrides": {
      "@akashic/akashic-cli-commons": "^0.14.18",
      "@akashic/akashic-cli-scan": "^0.16.26",
      "@akashic/playlog-client": "file:./vendor/playlog-client",
      "@akashic/akashic-gameview-web": "file:./vendor/akashic-gameview-web",
      "@akashic-extension/akashic-hover-plugin": "^3.2.2"
    }
  },
  "volta": {
    "node": "20.11.0",
    "pnpm": "8.14.1"
  }
}
