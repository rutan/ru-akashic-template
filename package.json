{
  "name": "@rutan/akashic-template",
  "version": "1.0.0",
  "author": "ru_shalm <ru_shalm@hazimu.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "akashic-sandbox ./game",
    "serve": "akashic serve --target-service nicolive ./game",
    "build": "run-s lint clean build:* scan",
    "build:webpack": "cross-env NODE_ENV=production webpack",
    "watch": "webpack -w",
    "assets": "run-s assets:* scan",
    "assets:mkdir": "mkdirp ./tmp && mkdirp src/assets && mkdirp game/assets/textures",
    "assets:updateAssets": "node ./scripts/convertAssets.js",
    "scan": "cd game && akashic scan asset",
    "clean": "run-p clean:*",
    "clean:script": "rimraf ./game/script/bundle.js",
    "storybook": "start-storybook -p 6006 -s ./game",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier -c ./src",
    "lint:tsc": "tsc --noEmit",
    "format": "run-s format:*",
    "format:eslint": "eslint --fix src",
    "format:prettier": "prettier --write ./src",
    "zip": "run-s build zip:*",
    "zip:mkdir": "mkdirp _dist && mkdirp ./tmp && rimraf ./tmp/build",
    "zip:export": "akashic export html --source ./game -o ./tmp/build --atsumaru --inject ./inject.html",
    "zip:copy": "cpx './public/**/*' ./tmp/build",
    "zip:deployment": "deployment-zip ./tmp/build",
    "prepare": "husky install",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@akashic-extension/akashic-hover-plugin": "^3.1.1",
    "@akashic-extension/resolve-player-info": "^1.1.1",
    "@akashic/akashic-engine": "^3.4.3",
    "@akashic/akashic-gameview-web": "file:./vendor/akashic-gameview-web",
    "@akashic/playlog-client": "file:./vendor/playlog-client",
    "@rutan/akashic-right-click-plugin": "^0.1.1",
    "eventmit": "^1.1.0"
  },
  "devDependencies": {
    "@akashic/akashic-cli": "^2.15.71",
    "@akashic/akashic-engine-standalone": "^3.4.3",
    "@akashic/akashic-sandbox": "^0.17.51",
    "@atsumaru/api-types": "atsumaru/api-types",
    "@kudohamu/storybook-akashic": "^6.3.13",
    "@rutan/deployment-zip": "^0.1.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "cpx2": "^4.2.0",
    "cross-env": "^7.0.3",
    "esbuild-loader": "^2.19.0",
    "eslint": "^8.20.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yaml-loader": "^0.8.0"
  },
  "resolutions": {
    "@akashic/playlog-client": "file:./vendor/playlog-client",
    "@akashic/akashic-gameview-web": "file:./vendor/akashic-gameview-web"
  }
}
