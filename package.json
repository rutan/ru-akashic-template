{
  "type": "module",
  "name": "@rutan/akashic-template",
  "version": "1.0.0",
  "author": "ru_shalm <ru_shalm@hazimu.com>",
  "license": "Unlicense",
  "private": true,
  "scripts": {
    "start": "node ./scripts/sandbox.cjs",
    "serve": "akashic serve --target-service nicolive ./game",
    "dev": "run-p start watch",
    "dev-serve": "run-p serve watch",
    "build": "run-s lint clean build:*",
    "build:vite": "cross-env NODE_ENV=production vite build",
    "build:scan": "pnpm run scan",
    "watch": "run-p watch:*",
    "watch:vite": "cross-env NODE_ENV=development vite build -w --mode development",
    "watch:scan": "cross-env JITI_ESM_RESOLVE=1 jiti ./scripts/scanAsset.ts -w",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "assets": "run-s assets:* scan",
    "assets:convertAssets": "cross-env JITI_ESM_RESOLVE=1 jiti ./scripts/convertAssets.ts",
    "scan": "run-s scan:*",
    "scan:assets": "cd game && akashic scan asset",
    "scan:format": "jiti ./scripts/formatGameJson.ts",
    "clean": "run-p clean:*",
    "clean:bundle": "jiti ./scripts/cleanBundle.ts",
    "lint": "run-s lint:*",
    "lint:biome": "biome check ./",
    "lint:tsc": "tsc --noEmit",
    "format": "biome check --write ./",
    "format-fix": "biome check --fix --unsafe ./",
    "deploy:web": "cross-env JITI_ESM_RESOLVE=1 jiti ./scripts/buildForWeb.ts && deployment-zip -c .deployment-zip.ts -m zip ./tmp/_build",
    "deploy:nicolive": "cross-env JITI_ESM_RESOLVE=1 jiti ./scripts/archiveForNicoLive.ts",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@akashic-extension/akashic-hover-plugin": "^3.2.3",
    "@akashic-extension/resolve-player-info": "^1.3.0",
    "@akashic/akashic-engine": "^3.21.0",
    "@rutan/frame-tween": "^0.7.0",
    "eventmit": "^3.0.0"
  },
  "devDependencies": {
    "@akashic/akashic-cli": "^3.0.10",
    "@akashic/akashic-cli-commons": "^1.0.0",
    "@akashic/akashic-cli-export": "^2.0.6",
    "@akashic/akashic-cli-scan": "^1.0.2",
    "@akashic/akashic-engine-standalone": "^3.21.0",
    "@akashic/akashic-sandbox": "^0.28.32",
    "@akashic/game-configuration": "^2.5.0",
    "@biomejs/biome": "^2.1.2",
    "@rutan/deployment-zip": "^0.5.0",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/html": "^9.0.17",
    "@storybook/html-vite": "^9.0.17",
    "@storybook/icons": "^1.4.0",
    "@types/node": "^24.1.0",
    "chokidar": "^4.0.3",
    "cross-env": "^10.0.0",
    "express": "^5.1.0",
    "free-tex-packer-core": "^0.3.4",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "npm-run-all2": "^8.0.4",
    "react": "^19.1.0",
    "storybook": "^9.0.18",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "packageManager": "pnpm@10.13.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild"
    ]
  }
}
